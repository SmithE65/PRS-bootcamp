# Keycloak admin credentials (used to initialize the dev Keycloak instance)
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# Keycloak image override (optional)
KEYCLOAK_IMAGE=quay.io/keycloak/keycloak:25.0.2

# Keycloak DB settings
KEYCLOAK_DB=postgres
KEYCLOAK_DB_NAME=keycloak
KEYCLOAK_DB_USER=keycloak
KEYCLOAK_DB_PASSWORD=password
KEYCLOAK_DB_PORT=5432

# PRS application DB settings
PRS_DB_NAME=app_db
PRS_DB_USER=app_user
PRS_DB_PASSWORD=app_password

# SQL Server
PRS_SQL_IMAGE=mcr.microsoft.com/mssql/server:2019-latest
PRS_SQL_SA_PASSWORD=YourStrong!Passw0rd
PRS_SQL_PID=Developer
PRS_SQL_PORT=1433
PRS_SQL_DB=AppDb